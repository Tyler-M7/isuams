<!-- Script for drop-down menus -->

<script language="javascript" type="text/javascript">
  if(document.images) {
    img1 = new Image();
    img2 = new Image();
    img1.src = "/ams/images/plus.gif";
    img2.src = "/ams/images/minus.gif";
  }
  function toggleLi(liid){
    var linkhead;
    linkhead = 'main' + liid;
    if(document.getElementById(liid).style.display == 'none'){
      document.getElementById(liid).style.display = 'block';
      document.getElementById(linkhead).innerHTML = '<img src="/ams/images/minus.gif" alt="-" border="0">';
    }else{
      document.getElementById(liid).style.display = 'none';
      document.getElementById(linkhead).innerHTML = '<img src="/ams/images/plus.gif" alt="+" border="0">';
    }
  }
</script>

<!-- Begin body text -->
<h2 style="text-align:left">Meeting Minutes</h2>

<p style="padding: 0px; margin: 0">
Here are the minutes from all the AMS meetings dating back to 
Fall of 2001 in order by date. The current school year is listed at the top. All other years
can be accessed by clicking the year of your choice.
</p>

